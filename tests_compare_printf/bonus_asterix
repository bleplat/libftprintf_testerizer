TEST("%-4.*s", 3, "truc")
TEST("%-4.*s", 39, "truc 2")
TEST("%-*.3s", 9, "truc 3")
TEST("%-4.*s", -39, "truc 2")
TEST("%-*.3s", -9, "truc 3")
TEST("%-*.*s", 9, 3, "truc 3")
TEST("%*.*s", 9, 3, "truc 3")
TEST("%*.*s", -9, 3, "truc 3")
TEST("%*.*s", 9, -3, "truc 3")
TEST("%*.*s", -9, -3, "truc 3")
TEST("%-*.*s", -9, -3, "truc 3")
