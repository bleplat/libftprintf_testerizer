// decimal
TEST("% d     %+d     %-d     %#d", 11, 12, 33, 55)
TEST("% d     %+d     %-d     %#d", -11, -12, -33, -55)
TEST("% .4d   %+.4d   %-.4d   %#.4d", -11, -12, -33, -55)
TEST("% .9d   %+.9d   %-.9d   %#.9d", -11, -12, -33, -55)
TEST("% 7.6d  %+7.6d  %-7.6d  %#7.6d", -11, -12, -33, -55)
TEST("% 7.1d  %+7.1d  %-7.1d  %#7.1d", -11, -12, -33, -55)
TEST("% 5.9d  %+5.9d  %-5.9d  %#5.9d", -11, -12, -33, -55)
TEST("% 7.6d  %+7.6d  %-7.6d  %#7.6d", 111, 54612, 45633, 45655)
TEST("% 7.1d  %+7.1d  %-7.1d  %#7.1d", 6511, 6512, 6533, 6555)
TEST("% 5.9d  %+5.9d  %-5.9d  %#5.9d", 6511, 66512, 56633, 445555)
