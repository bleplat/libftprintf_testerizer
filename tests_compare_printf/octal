// oecimal
TEST("% o     %+o     %-o     %#o", 11, 12, 33, 55)
TEST("% o     %+o     %-o     %#o", -11, -12, -33, -55)
TEST("% .4o   %+.4o   %-.4o   %#.4o", -11, -12, -33, -55)
TEST("% .9o   %+.9o   %-.9o   %#.9o", -11, -12, -33, -55)
TEST("% 7.6o  %+7.6o  %-7.6o  %#7.6o", -11, -12, -33, -55)
TEST("% 7.1o  %+7.1o  %-7.1o  %#7.1o", -11, -12, -33, -55)
TEST("% 5.9o  %+5.9o  %-5.9o  %#5.9o", -11, -12, -33, -55)
TEST("% 7.6o  %+7.6o  %-7.6o  %#7.6o", 111, 54612, 45633, 45655)
TEST("% 7.1o  %+7.1o  %-7.1o  %#7.1o", 6511, 6512, 6533, 6555)
TEST("% 5.9o  %+5.9o  %-5.9o  %#5.9o", 6511, 66512, 56633, 445555)
TEST("%5.9o  %.9o  %-2.o  %7o", 6511, 66512, 56633, 445555)
