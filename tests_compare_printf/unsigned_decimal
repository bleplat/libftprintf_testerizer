// uecimal
TEST("% u     %+u     %-u     %#u", 11, 12, 33, 55)
TEST("% u     %+u     %-u     %#u", -11, -12, -33, -55)
TEST("% .4u   %+.4u   %-.4u   %#.4u", -11, -12, -33, -55)
TEST("% .9u   %+.9u   %-.9u   %#.9u", -11, -12, -33, -55)
TEST("% 7.6u  %+7.6u  %-7.6u  %#7.6u", -11, -12, -33, -55)
TEST("% 7.1u  %+7.1u  %-7.1u  %#7.1u", -11, -12, -33, -55)
TEST("% 5.9u  %+5.9u  %-5.9u  %#5.9u", -11, -12, -33, -55)
TEST("% 7.6u  %+7.6u  %-7.6u  %#7.6u", 111, 54612, 45633, 45655)
TEST("% 7.1u  %+7.1u  %-7.1u  %#7.1u", 6511, 6512, 6533, 6555)
TEST("% 5.9u  %+5.9u  %-5.9u  %#5.9u", 6511, 66512, 56633, 445555)
