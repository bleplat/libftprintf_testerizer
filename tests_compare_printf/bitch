// tests from moulitest
TEST("ju", 4999999999)
TEST("%llX", 4294967296)
TEST("%llx", 9223372036854775807)
TEST("%llx", 9223372036854775808)
TEST("%#x", 0)
TEST("@moulitest: %5.x %5.0x", 0, 0)
TEST("%.2c", NULL)
TEST("@moulitest: %c", 0)
TEST("@moulitest: %5.o %5.0o", 0, 0)
TEST("@moulitest: %.o %.0o", 0, 0)
TEST("@moulitest: %5.d %5.0d", 0, 0)
TEST("@moulitest: %.d %.0d", 0, 0)
TEST("%010s is a string", "this")
TEST("%05c", 42)
TEST("%5+d", 42)
//TEST("%hhld", 128)
//TEST("@main_ftprintf: %####0000 33..1..#00d\n", 256)
//TEST("@main_ftprintf: %###-#0000 33...12..#0+0d", 256)
TEST("%p", (void*)0)
//TEST("[ %C ]", L'Á±≥')
//TEST("[ %C, %C ]", 0, L'Á±≥')
//TEST("%hhC, %hhC", 0, L'Á±≥')
TEST("%.p, %.0p", (void*)0, (void*)0)
TEST("%*d", -5, 42)
TEST("%10d", 42)
TEST("%S", L"ABCDEFGHIJKLMNOPQRSTUVWXYZ")
TEST("%4.9S", L"ABCDEFGHIJKLMNOPQRSTUVWXYZ")
//TEST("%4S", L"ÊM-M-^QÊM-^XØ‰∏M-ÂM-^O™ÁM-^L´„M-M-^B")
//TEST("%4.S", L"ÊM-M-^QÊM-^XØ‰∏M-ÂM-^O™ÁM-^L´„M-M-^B")
//TEST("%4.0S", L"ÊM-M-^QÊM-^XØ‰∏M-ÂM-^O™ÁM-^L´„M-M-^B")
//TEST("%4.1S", L"ÊM-M-^QÊM-^XØ‰∏M-ÂM-^O™ÁM-^L´„M-M-^B")
//TEST("%4.2S", L"ÊM-M-^QÊM-^XØ‰∏M-ÂM-^O™ÁM-^L´„M-M-^B")
