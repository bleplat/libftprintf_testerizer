// xecimal
TEST("% x     %+x     %-x     %#x", 11, 12, 33, 55)
TEST("% x     %+x     %-x     %#x", -11, -12, -33, -55)
TEST("% .4x   %+.4x   %-.4x   %#.4x", -11, -12, -33, -55)
TEST("% .9x   %+.9x   %-.9x   %#.9x", -11, -12, -33, -55)
TEST("% 7.6x  %+7.6x  %-7.6x  %#7.6x", -11, -12, -33, -55)
TEST("% 7.1x  %+7.1x  %-7.1x  %#7.1x", -11, -12, -33, -55)
TEST("% 5.9x  %+5.9x  %-5.9x  %#5.9x", -11, -12, -33, -55)
TEST("% 7.6x  %+7.6x  %-7.6x  %#7.6x", 111, 54612, 45633, 45655)
TEST("% 7.1x  %+7.1x  %-7.1x  %#7.1x", 6511, 6512, 6533, 6555)
TEST("% 5.9x  %+5.9x  %-5.9x  %#5.9x", 6511, 66512, 56633, 445555)
